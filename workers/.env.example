# Copy this file to .env and fill in your values

# Email service configuration
EMAIL_API_KEY=your_email_service_api_key_here
EMAIL_API_URL=https://api.emailservice.com/send

# JWT secret for token signing (generate a secure random string)
JWT_SECRET=your_jwt_secret_here

# Development settings
ENVIRONMENT=development

# Allowed origins (comma-separated for production)
ALLOWED_ORIGINS=https://stillmind.app,https://www.stillmind.app